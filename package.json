  "license": "MIT",
  "author": "Neyunse",
  "main": "style.js",
  "homepage": "https://kagaricssdoc.netlify.app",
  "keywords": [
    "css",
    "front-end",
    "framework",
    "web",
    "(csc) Cascading style component"
  ],
  "sass": "src/scss/common.scss",
  "style": "dist/css/common.min.css",
  "module": "style.js",
  "bugs": {
    "url": "https://github.com/KagariSoft/KagariCSS/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KagariSoft/KagariCSS.git"
  },
  "scripts": {
    "css:watch": "nodemon -e scss -x \"yarn css:build\"",
    "css:build": "node-sass --include-path scss src/scss/common.scss dist/css/common.min.css --output-style compressed"
  },
  "devDependencies": {
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.16"
  },
  "files": [
    "dist/css/*.css"
  ],
